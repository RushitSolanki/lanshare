{
  "productName": "LanShare",
  "identifier": "com.example.lanshare",
  "version": "0.1.0",
  "build": {
    "beforeBuildCommand": "",
    "beforeDevCommand": "",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "LanShare",
        "width": 900,
        "height": 600,
        "resizable": false,
        "minWidth": 900,
        "minHeight": 600,
        "maxWidth": 900,
        "maxHeight": 600,
        "maximizable": false,
        "fullscreen": false,
        "decorations": true,
        "alwaysOnTop": false
      }
    ],
    "security": {
      "csp": null
    },
    "withGlobalTauri": true
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",   
      "icons/icon.ico"
      ]}
}